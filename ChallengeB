live_loop :one do
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  sample :drum_snare_soft
  sleep 2
  stop
  
end

